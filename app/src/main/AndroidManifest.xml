        <activity android:name=".Navigation.Categoryscreen"/>
        <activity android:name=".Activity.RegisterActivity"/>
        <activity android:name=".Activity.RegisterActivity1"/>
        <activity android:name=".Activity.PropertyListingActivity"></activity>
        <activity android:name=".Activity.PropertyDetailsActivity"></activity>
        <activity android:name=".Rentscreen" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.PostPropertyActivity" />
        <activity android:name=".Activity.MainActivity" />

        <activity android:name=".SplashScreen">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>